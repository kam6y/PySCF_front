{
  "name": "pyscf-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for PySCF Native App - Quantum Chemistry Calculations",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "type-gen": "openapi-typescript ../src/api-spec/openapi.yaml --output src/types.ts",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "setup-config": "node ../scripts/setup-mcp-config.js"
  },
  "keywords": [
    "mcp",
    "claude",
    "pyscf",
    "quantum-chemistry",
    "molecular-chemistry"
  ],
  "author": "PySCF Native App",
  "license": "Apache-2.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axios": "^1.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "openapi-typescript": "^7.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}