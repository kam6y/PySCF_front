{
  "name": "pyscf_native_app",
  "version": "0.0.1",
  "description": "A quantum chemical calculationn app.",
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:webpack dev:electron",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on ./dist/index.html ./dist/main.js && electronmon .",
    "build": "cross-env NODE_ENV=\"production\" webpack --progress",
    "package": "npm run build && electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kam6y/PySCF_native_app.git"
  },
  "keywords": [],
  "author": "",
  "electronmon": {
    "patterns": [
      "dist/**/*"
    ]
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kam6y/PySCF_native_app/issues"
  },
  "homepage": "https://github.com/kam6y/PySCF_native_app#readme",
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "^37.2.4",
    "electron-builder": "^26.0.12",
    "electronmon": "^2.0.3",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.4",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "3dmol": "^2.5.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "build": {
    "appId": "com.example.pyscf-native-app",
    "productName": "PySCFNativeApp",
    "files": [
      "dist/**/*"
    ],
    "extraResources": [
      {
        "from": "python_dist",
        "to": "python_dist",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "src/assets/icon/win/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "src/assets/icon/mac/Pyscf_front.icns"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
